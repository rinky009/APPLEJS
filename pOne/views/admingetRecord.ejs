<h1>Details of Registration Form is <%= student.length %></h1> <br><br>
<a href='/Formadmin'>Add admin</a><br>
    <table border="1">
        <tr>
            
            <th>aid</th>
            <th>name</th>
            <th>email</th>
            <th>password</th>
             <th>panno</th>


        </tr>
        <% for(var i=0; i < JSON.stringify(student.length); i++) { %>
            <tr>
                
                 <td>
                    <%=student[i].aid%>
                </td>
                <td>
                    <%=student[i].name%>
                </td>
                <td>
                    <%=student[i].email%>
                </td>
                <td>
                    <%=student[i].password%>
                </td>
               <td>
                    <%=student[i].panno%>
                </td>
                <td>
                    <a href='/admingetRecordid/<%=student[i].aid%>'>Update</a>
                    <a href='/admindeleteRecord/<%=student[i].aid%>'onclick ="return confirm (' Are you sure')">Delete</a>
                </td>
            </tr>
            <% } %>
    </table>
